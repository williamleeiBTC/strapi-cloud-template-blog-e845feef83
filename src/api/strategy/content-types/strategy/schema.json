{
  "kind": "collectionType",
  "collectionName": "strategies",
  "info": {
    "singularName": "strategy",
    "pluralName": "strategies",
    "displayName": "Strategy",
    "description": "Strategy collection for investment strategies"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": ["images"]
    },
    "apyEstimate": {
      "type": "string",
      "required": false
    },
    "points": {
      "type": "text"
    },
    "prospectusLink": {
      "type": "string",
      "regex": "^(http|https)://",
      "required": false
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    }
  }
} 